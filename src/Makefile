OBJS = uimain.o history.o tokenizer.o
# compiler flags. -g for debug, -03 for optimization
CFLAGS=-g -03

# first target is built by default
all: tokenizer


# $@=target, $^=prerequisites
tokenizer: $(OBJS)
		cc -o tokenizer $(CFLAGS) $(OBJS)

$(OBJS): tokenier.h history.history

# deletes files generated by compilation
clean:
		rm -f *.0 tokenizer

run: tokenizer
		./tokenizer